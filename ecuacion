#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

void main(){
	float a,b,c,d,e,f,g,x,y;
	cout<<"ingrese a: ";
	cin>>a;
	cout<<"ingrese b: ";
	cin>>b;
	cout<<"ingrese c: ";
	cin>>c;
	cout<<"ingrese d: ";
	cin>>d;
	cout<<"ingrese e: ";
	cin>>e;
	cout<<"ingrese f: ";
	cin>>f;
	cout<<"ingrese g: ";
	cin>>g;
	y=(d*c-a*f)/(d*b-a*e);
	x=(c-b*y)/a;
	cout<<"x="<<x<<endl;
	cout<<"y="<<y;
	getch();
}
